{
  "name" : "Paramanager",
  "version" : "1.1",
  "description" : "For your query params.",
  "permissions": [
    "browsingData",
    "activeTab"
  ],
  "background" : {
    "scripts" : ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
     "default_icon": "fms.png"
  },
  "manifest_version": 2
}