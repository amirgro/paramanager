{
  "name" : "Parambola",
  "version" : "1.1",
  "description" : "For your query params.",
  "permissions": [
    "browsingData",
    "activeTab"
  ],
  "icons": { 
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png" 
  },
  "background" : {
    "scripts" : ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
     "default_icon": "profiles/fms/fms.png"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "manifest_version": 2
}